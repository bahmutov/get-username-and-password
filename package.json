{
  "name": "get-username-and-password",
  "version": "1.2.0",
  "description": "Promise-returning CLI function that asks the user the username and password to login, or grabs from environment",
  "main": "get-username-and-password.js",
  "scripts": {
    "test": "eslint . && npm run style",
    "style": "jscs *.js --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bahmutov/get-username-and-password.git"
  },
  "keywords": [
    "cli",
    "prompt",
    "ask",
    "username",
    "password",
    "login",
    "promise"
  ],
  "author": "Gleb Bahmutov <gleb.bahmutov@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bahmutov/get-username-and-password/issues"
  },
  "homepage": "https://github.com/bahmutov/get-username-and-password#readme",
  "pre-commit": "npm test",
  "post-commit": "npm version",
  "eslintConfig": {
    "env": {
      "node": true
    },
    "rules": {
      "no-unused-expressions": 2,
      "no-undef": 2
    },
    "globals": {
      "la": true
    }
  },
  "dependencies": {
    "bluebird": "2.9.34",
    "inquirer": "0.9.0"
  },
  "devDependencies": {
    "eslint": "1.1.0",
    "jscs": "2.1.0",
    "pre-git": "0.6.2"
  }
}
